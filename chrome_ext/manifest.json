{
   "name" : "News Speak"
 , "version" : "0.0.1"
 , "description" : "Chat about the news as you read it"
 , "browser_action" : {
       "default_icon" : "icon.png"
		 , "default_popup" : "popup.html"
		 , "default_title" : "News Speak"
   }
 , "icons" : { "128" : "icon_128.png" }
 , "permissions": [ 
        "tabs",
        "https://facebook.com/connect/*"
    ]
 , "content_scripts" : [
   {
     "matches": ["http://www.foxnews.com/*"]
    ,"run_at" : "document_end"
    ,"css" : ["css/chat.css", "css/screen.css", "css/bar.css"]
    , "js" : ["js/jquery.js", "fox_scrape.js"]
	 }]
}
